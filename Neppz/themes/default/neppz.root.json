{	
	"name": "neppz!",
	"author": ["Geri Borbás", "Nurdogan Karaman"],
	"comment": "Created by Geri Borbás (https://twitter.com/_eppz) Edited by Nurdogan Karaman (https://twitter.com/nurdiloyd) © 2022 Creative Commons 4.0 BY-NC",
	"include": "neppz.regions.using.json",
	"settings":
	[
		{
			// Text
			"name": "Source",
			"scope": [ "source" ],
			"settings": { "foreground": "#dddcd3" }
		},
		{
			// Text
			"name": "Keyword (operator)",
			"scope": [ "keyword.operator" ],
			"settings": { "foreground": "#b9b9b5" }
		},
		{
			// Text
			"name": "Punctuation",
			"scope": [ "punctuation" ],
			"settings": { "foreground": "#9c9d9d" }
		},
		{
			// Text
			"name": "Punctuation (parenthesis)",
			"scope":
			[
				"punctuation.parenthesis"
			],
			"settings": { "foreground": "#85878a" }
		},
		{
			// Text
			"name": "Punctuation (square bracket)",
			"scope":
			[
				"punctuation.squarebracket"
			],
			"settings": { "foreground": "#72757b" }
		},
		{
			// Text
			"name": "Punctuation (curly brace)",
			"scope": [ "punctuation.curlybrace" ],
			"settings": { "foreground": "#63676f" }
		},
		{
			// Text
			"name": "Comment",
			"scope":
			[
				"comment",
				"comment punctuation",
				"comment punctuation.parenthesis",
				"comment punctuation.squarebracket",
			 	"comment punctuation.curlybrace",
				"comment keyword",
				"comment entity",
				"comment variable",
				"comment string"
			],
			"settings": { "foreground": "#575b65" }
		},
		{
			// Literals
			"name": "Literals (Constant)",
			"scope": [ "constant" ],
			"settings": { "foreground": "#6074d2" }
		},
		{
			// Literals
			"name": "Literals (null)",
			"scope": [ "constant.language.null" ],
			"settings": { "foreground": "#58649a" } // "Literals (Constant)" - 1
		},
		{
			// Literals
			"name": "Literals (Number)",
			"scope": [ "constant.numeric" ],
			"settings": { "foreground": "#609ad2" }
		},
		{
			// Literals
			"name": "Literals (String)",
			"scope":
			[
				"string.quoted.double",
				"string.quoted.double punctuation"
			],
			"settings": { "foreground": "#60c0d2" }
		},
		{
			// Literals
			"name": "Literals (Escape)",
			"scope": [ "constant.character.escape" ],
			"settings": { "foreground": "#588e9a" } // "Literals (String)" - 1
		},
		{
			// Literals
			"name": "Literals (Char)",
			"scope":
			[
				"string.quoted.single",
				"string.quoted.single punctuation"
			],
			"settings": { "foreground": "#60d2be" }
		},
		{
			// Functions
			"name": "Functions",
			"scope":
			[
				"entity.name.function",
				"entity.name.method",
				"keyword.other.get",
				"keyword.other.set",				
				"keyword.other.add",				
				"keyword.other.remove"		
			],
			"settings": { "foreground": "#318D75" }
		},
		{
			// Functions (support)
			"name": "Functions (UnityEngine)",
			"scope": [ "support.function.UnityEngine" ],
			"settings": { "foreground": "#667E77" } // - 1
		},
		{
			// Functions (support)
			"name": "Functions (UnityEditor)",
			"scope": [ "support.function.UnityEditor" ],
			"settings": { "foreground": "#497b6c" } // - 1
		},
		{
			// Functions (support)
			"name": "Functions (System)",
			"scope": [ "support.function.System" ],
			"settings": { "foreground": "#405651" } // - 2
		},
		{
			// Types
			"name": "Types",
			"comment": "Including classes, delegates, enums, interfaces, structs.",
			"scope":
			[
				"storage.type",
				"entity.name.type.type-parameter"
			],
			"settings": { "foreground": "#583E3A" }
		},
		{
			// Types
			"name": "Types (built-in)",
			"scope": [ "keyword.type" ],
			"settings": { "foreground": "#583E3A" } // - 1
		},
		{
			// Types
			"name": "Types (<>)",
			"scope": [ "punctuation.definition.typeparameters" ],
			"settings": { "foreground": "#583E3A" } // - 2
		},
		{
			// Types (support)
			"name": "Types (UnityEngine)",
			"scope": [ "support.type.unity.UnityEngine" ],
			"settings": { "foreground": "#6C4A45" } // - 1
		},
		{
			// Types (support)
			"name": "Types (UnityEditor)",
			"scope": [ "support.type.unity.UnityEditor" ],
			"settings": { "foreground": "#6C4A45" } // - 1
		},
		{
			// Types (support)
			"name": "Types (System)",
			"scope": [ "support.type.System" ],
			"settings": { "foreground": "#6C4A45" } // - 2
		},
		{
			// Modifiers like "public", "private"
			"name": "Modifiers",
			"scope": [ "storage.modifier" ],
			"settings": { "foreground": "#3F3D3D" } // - 2
		},
		{
			// Modifiers reference of user implemented classes
			"name": "Modifiers (other)",
			"scope": [ "storage.type"],
			"settings": { "foreground": "#3F3D3D" } // - 2
		},
		{
			// Types (declarations)
			"name": "Types (declarations)",
			"comment": "Including classes, delegates, enums, interfaces, structs.",
			"scope": [ "entity.name.type" ],
			"settings": { "foreground": "#8068C2" }
		},
		{
			// Keywords
			"name": "Keywords",
			"scope":
			[
				"keyword.control",
				"keyword.operator.conditional",
				"keyword.query"
			],
			"settings": { "foreground": "#8A3863" }
		},
		{
			// Keywords (other)
			"name": "Keywords (other)",
			"scope": [ "keyword.other" ],
			"settings": { "foreground": "#8D4A3F" } // - 1
		},
		{
			// Preprocessor
			"name": "Preprocessor",
			"scope":
			[
				"meta.preprocessor",
				"meta.preprocessor punctuation",
				"meta.preprocessor punctuation.parenthesis",
				"meta.preprocessor punctuation.squarebracket",
			 	"meta.preprocessor punctuation.curlybrace",
				"meta.preprocessor keyword"
			],
			"settings": { "foreground": "#2B2F38" } // - 2
		},
		{
			// Preprocessor
			"name": "Preprocessor (entities)",
			"scope":
			[
				"meta.preprocessor entity",
				"meta.preprocessor entity.name.method",				
				"meta.preprocessor entity.name.function",				
				"meta.preprocessor entity.name.variable",
				"meta.preprocessor variable",
				"meta.preprocessor string"
			],
			"settings": { "foreground": "#363B48" } // - 1
		},
		{
			// Variables
			"name": "Variables",
			"scope":
			[
				"variable.other.readwrite",
				"variable.other.object",
				"variable.other.alias",
				"entity.name.variable"	
			],
			"settings": { "foreground": "#9F9C8C" }
		},
		{
			// Variables (local)
			"name": "Variables (local)",
			"scope":
			[
				"variable.other.object.property",
				"entity.name.variable.local",
				"keyword.other.base",
				"keyword.other.this"
			],
			"settings": { "foreground": "#66655C" } // - 1
		},
		{
			// Variables (support).
			"name": "Variables (UnityEngine)",
			"scope": [ "support.variable.UnityEngine" ],
			"settings": { "foreground": "#FFCC00" } // - 1
		},
		{
			// Variables (support)
			"name": "Variables (UnityEditor)",
			"scope": [ "support.variable.UnityEditor" ],
			"settings": { "foreground": "#FFCC00" } // - 1
		},
		{
			// Variables (support)
			"name": "Variables (System)",
			"scope": [ "support.variable.System" ],
			"settings": { "foreground": "#FFCC00" } // - 2
		}
	]
}
